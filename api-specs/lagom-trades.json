{
  "openapi": "3.0.1",
  "info": {
    "title": "Lagom Trades",
    "description": "Lagom Trades",
    "version": "v1",
    "contact": {}
  },
  "servers": [
    {
      "url": "https://api.blocksniper.io/api/v1/trades"
    }
  ],
  "paths": {
    "/public/trades": {
      "get": {
        "tags": ["public-trade-controller"],
        "summary": "get recent trades all pair",
        "operationId": "getRecentTradesAllPais",
        "parameters": [
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 20
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResponseDtoListRecentTradesInPairResponseDto"
                }
              }
            }
          }
        }
      }
    },
    "/public/trades/{pairSymbol}": {
      "get": {
        "tags": ["public-trade-controller"],
        "summary": "get recent trades by pair",
        "operationId": "getRecentTradesByPair",
        "parameters": [
          {
            "name": "pairSymbol",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 20
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResponseDtoListPublicTradeResponseDto"
                }
              }
            }
          }
        }
      }
    },
    "/history/trade": {
      "get": {
        "tags": ["history-trade-controller"],
        "summary": "get trading history",
        "operationId": "getTradingHistory",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 1
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 20
            }
          },
          {
            "name": "side",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["ASK", "BID"]
            }
          },
          {
            "name": "pair",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "default": ""
            }
          },
          {
            "name": "baseCurrency",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "quoteCurrency",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "startTime",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int64",
              "default": 0
            }
          },
          {
            "name": "endTime",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int64",
              "default": 0
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/PageResponseDtoMyTradeResponseDto"
                }
              }
            }
          }
        },
        "security": [
          {
            "Bearer Authentication": []
          }
        ]
      }
    },
    "/history/trade/{pairSymbol}": {
      "get": {
        "tags": ["history-trade-controller"],
        "summary": "get trading history by symbol",
        "operationId": "getTradingHistoryBySymbol",
